#include<stdio.h>
#include<string.h>
int main()
{
    int num1,num2,result;
    scanf("%d",&num1);
    scanf("%d",&num2);
    printf("Enter the number between 1 to 4\n");
    int op;
    scanf("%d",&op);
    switch(op)
    {
    case 1:
        result=num1+num2;
        printf("%d",result);
        break;
    case 2:
        result=num1-num2;
        printf("%d",result);
        break;
    case 3:
        result=num1*num2;
        printf("%d",result);
        break;
    case 4:
        result=num1%num2;
        printf("%d",result);
        break;
    default:
        printf("No value matched:");
    }

}
