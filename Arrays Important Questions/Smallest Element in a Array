#include<stdio.h>
#include<string.h>
extern int i=0;
int First_Ele(int n,int arr[10])
{
     int max=arr[0];
     for(i=0;i<n;i++)
     {
         if(arr[i]<max)
         {
             max=arr[i];

         }

     }
     printf("%d",max);

}
int main()
{

    int arr[10];
    int n;
    printf("Enter the Number of Element;\n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
    scanf("%d",&arr[i]);
    }
    First_Ele(n,arr);
}
