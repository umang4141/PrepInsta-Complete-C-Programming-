#include <stdio.h>
int main()
{
    int a = 5, b = 10;
    printf("a=5, b=10\n");
    printf("%d == %d is %d \n", a, b, a == b);
    printf("%d != %d is %d \n", a, b, a != b);
    printf("%d > %d is %d \n", a, b, a > b);
    printf("%d < %d is %d \n", a, b, a < b); printf("%d >= %d is %d \n", a, b, a >= b);
    printf("%d <= %d is %d \n", a, b, a <= b);
    return 0;
}


OUTPUT:

a=5, b=10
5 == 10 is 0 
5 != 10 is 1 
5 > 10 is 0 
5 < 10 is 1 5 >= 10 is 0 
5 <= 10 is 1
